 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header, render_field -%}\n\n<!-- Outer container for the print format with some styling -->\n<div class=\"print-format .print-format-preview\" style=\"padding-left: 0px; \">\n   <!-- Page Header Section -->\n\n\n\n\n   <div class=\"page-header\">\n      {{ add_header(0, 1, doc, letter_head, no_letterhead,page_num ) }}\n   </div>\n   <!-- Page Footer Section -->\n   <div class=\"page-footer\">\n      {{footer}}\n      {%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\n      <div style=\"display:block;\"> {{page_num}} of {{max_pages}}</div>\n      {%- endmacro -%}\n\n   </div>\n   <!-- Table for the entire print format -->\n\n   <table style=\"width:100%;margin-top:-12px;\">\n      <!-- Table Header Section -->\n      <thead>\n         <tr>\n            <td>\n               <!-- Placeholder for the fixed-position header -->\n               <div class=\"page-header-space\"></div>\n            </td>\n         </tr>\n      </thead>\n      <!-- Table Body Section -->\n\n      <div style=\"border-left: 2px solid black;\">\n         <tbody\n            style=\"width: 103.2%;margin-top: -11px;margin-left:-11px;border-left: 2px solid black; font-size: 10px;\">\n            <td>\n               <table\n                  style=\"width: 103.1%; border-collapse: collapse; border: 2px solid black; width: 103%;margin-top: 0px;margin-left:-11px \">\n                  <tr>\n                     <td style=\"text-align: center; border-left: 2px solid black; border-bottom: 2px solid black;\">\n                        <b><u>DELIVERY CHALLAN</u></b>\n                     </td>\n                  </tr>\n               </table>\n               <div style='display:flex; width: 103.1%; margin-top: 0px; margin-left:-11px'>\n                  <div style='width:50%; border-left: 2px solid black;'>\n                     <table style=\"display: block; width:100%;\" class='address-table'>\n                        {% if doc.shipping_address_name %}\n                        {% set customer_shipping =\n                        frappe.db.get_value('Address', doc.shipping_address_name, ['address_title', 'address_type',\n                        'address_line1', 'address_line2', 'city', 'country', 'state','pincode',\n                        'gstin','custom_contact_person_name','custom_mobile_no']) %}\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 35%; padding: 0 !important;\">Company Name</td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style='padding: 0 !important;'><b>{{customer_shipping[0] or ''}}</b></td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 35%; padding: 0 !important;\">Address</td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\">{{doc.shipping_address}}</td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 35%; padding: 0 !important;\">Contact Person</td>\n                           <td style=\"width: 3%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\">{{customer_shipping[9] or ''}}</td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 35%; padding: 0 !important;\"> Tel No. </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\">{{customer_shipping[10] or ''}}</td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 35%; padding: 0 !important;\"> Buyer's GST No. </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\">{{customer_shipping[8] or ''}}</td>\n                        </tr>\n                        {% else %}\n                        {% set customer_billing =\n                        frappe.db.get_value('Address', doc.customer_address, ['address_title', 'address_type',\n                        'address_line1', 'address_line2', 'city', 'country', 'state','pincode',\n                        'gstin','custom_contact_person_name','custom_mobile_no']) %}\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 35%; padding: 0 !important;\">Company Name</td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\"><b>{{customer_billing[0] or ''}}</b></td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 35%; padding: 0 !important;\">Address</td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\">{{doc.address_display or ''}}</td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 35%; padding: 0 !important;\">Contact Person</td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\">{{customer_billing[9] or ''}}</td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 35%; padding: 0 !important;\"> Tel No. </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\">{{customer_billing[10] or ''}}</td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 35%; padding: 0 !important;\"> Buyer's GST No. </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\">{{customer_billing[8] or ''}}</td>\n                        </tr>\n                        {% endif %}\n                     </table>\n                  </div>\n                  <div style='width:50%; border-left: 2px solid black; border-right: 2px solid black;'>\n                     <table style=\"display: block; width:100%;\" class='address-table'>\n                        {% set sales_person =\n                           frappe.db.get_value('Sales Invoice', doc.custom_sales_invoice_no, ['custom_sales_person']) %}\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 45%; padding: 0 !important;\"> DC No. </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\"> {{doc.name}} </td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 45%; padding: 0 !important;\"> Dated </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\"> {{ doc.get_formatted(\"posting_date\") or ' '}} </td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 45%; padding: 0 !important;\"> PO No. </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\">{{doc.po_no or ''}} </td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 45%; padding: 0 !important;\"> SO No. </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\"> </td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 45%; padding: 0 !important;\"> Reference </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\"> {{sales_person or \" \" }} </td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 45%; padding: 0 !important;\"> Place of Supply </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\"> {{doc.place_of_supply or ''}} </td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 45%; padding: 0 !important;\"> PAN No. </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\"> </td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"width: 45%; padding: 0 !important;\"> GST No. </td>\n                           <td style=\"width: 5%; padding: 0 !important;\">:</td>\n                           <td style=\"padding: 0 !important;\"> {{doc.company_gstin or ''}} </td>\n                        </tr>\n                     </table>\n                  </div>\n               </div>\n               <table\n                  style=\"width: 103%;margin-top:0px;margin-left:-11px; border-collapse: collapse; border: 2px solid black;\">\n                  <tr>\n                     <td>\n                        <table style=\"width: 103.1%;margin-top: -11px;margin-left: -10.7px; \">\n                           <tr>\n                              <th style=\"border: 2px solid black; width:10%;\">Sr.No.</th>\n                              <th style=\"border: 2px solid black; width:40%;\">Name of Product/Service</th>\n                              <th style=\"border: 2px solid black;\">HSN/SAC</th>\n                              <th style=\"border: 2px solid black;\">Qty</th>\n                              <th style=\"border: 2px solid black;\">Unit</th>\n                           </tr> {% for item in doc.items %} <tr>\n                              <td style=\"border: 2px solid black;\">{{item.idx}}</td>\n                              <td style=\"border: 2px solid black;\">{{item.custom_item_description or item.item_name}}\n                              </td>\n                              <td style=\"border: 2px solid black;\">{{item.gst_hsn_code or \"\"}}</td>\n                              <td style=\"border: 2px solid black;\">{{item.qty}}</td>\n                              <td style=\"border: 2px solid black;\">{{item.uom}}</td>\n                           </tr> {% endfor %}\n                           <tr style=\"border-bottom: 2px solid black;\">\n                              <td colspan=5 style='text-align:right'><b>Grand Total </b> {{doc.total_qty}} Units</td>\n                           </tr>\n                        </table>\n                     </td>\n                  </tr>\n                  <tr style=\"border-right: 2px solid black;\">\n                     <!--<td>-->\n                     <!--   <table style=\"width: 100%;\">-->\n                     <!--      <tr>-->\n                     <td>\n                        <b>Terms & Conditions</b>\n                        {{doc.terms or \" \"}}\n                     </td>\n                     <!--      </tr>-->\n                     <!--   </table>-->\n                     <!--</td>-->\n                  </tr>\n               </table>\n               <div style='display:flex;  width: 103%; margin-top: -1px; margin-left:-11px '>\n                  <div style='width:33.33%;border-left:2px solid black;border-top:2px solid black;'>\n                     <table style='width:100%;'>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"padding: 0 0 3px 0 !important;\">Dispatch Th:</td>\n                        </tr>\n                        <tr style=\"padding: 0 0 3px 0 !important;\">\n                           <td style=\"padding: 0 !important;\">Dispatch No:</td>\n                        </tr>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"padding: 0 !important;\">Dispatch Mode:</td>\n                        </tr>\n                     </table>\n                  </div>\n                  <div\n                     style='width:33.33%;text-align:center;border-top:2px solid black;border-left:2px solid black;border-right:2px solid black; display:flex;flex-direction:column;justify-content:space-between;'>\n                     <table style='width:100%; display:block;'>\n                        <tr style=\"padding: 0 !important;\">\n                           <td style=\"padding: 0 0 3px 0 !important;\">Received the above goods in good condition.</td>\n                        </tr>\n                        <tr>\n                           <td>Invoice accepted and admitted for payment</td>\n                        </tr>\n                     </table>\n                     <br>\n                     <br>\n                     <p style=\"margin: 0;\">Recipient's Signature</p>\n                  </div>\n                  <div\n                     style='width:33.33%;text-align:center;border-top:2px solid black;border-right:2px solid black; display:flex;flex-direction:column;justify-content:space-between;'>\n                     <table style='width:100%;display:block;'>\n                        <tr>\n                           <td>MANTRA SOFTECH (INDIA) PRIVATE LIMITED</td>\n                        </tr>\n                     </table>\n                     <p style=\"\">Authorized Signatory</p>\n                  </div>\n               </div>\n               <div style=\"width: 103%;margin-top:0px;margin-left:-11px; border:2px solid black; text-align:center;\">\n                  <p>\n                     Registered Office Address: B-203, 2nd Floor, Shapath Hexa, Near Gujarat High Court, Opp. Kargil\n                     Petrol Pump,S.G. Highway,Sola,\n                     Ahmedabad, Gujarat - 380060, India\n                  </p>\n               </div>\n               <!-- Repeat the same structure for additional table sections if needed -->\n               {% set temp = namespace(name=\"no_serial\",repeat=1) %}\n               {% for row in doc.items %}\n               {% if row.serial_and_batch_bundle %}\n               {% set temp.name = \"yes_serial\" %}\n               {% endif %}\n               {% endfor %}\n               {% if temp.name == \"yes_serial\" %}\n               <div class=\"page-break\">\n               </div>\n               <!--<div class=\"page-header-space\" style=\"margin-bottom:115px;\"></div>-->\n               {% endif %}\n               <table\n                  style=\" border-collapse: collapse; width: 103%;margin-left:-11px; position : relative; top : -3px \">\n                  {% set row_count = 0 %}\n                  {% for item in doc.items %}\n                  {% if item.serial_and_batch_bundle %}\n                  <tr style=\" \">\n                     <td colspan=\"2\" style=\"background-color:#c5c5c5;color:black;padding:5px;border:1px solid black;\">\n                        <div style=\"display:flex;justify-content:space-between;\">\n                           <div>{{ item.item_code }}</div>\n                           <div style=\"text-align:right;\">{{ item.qty }} Units</div>\n                        </div>\n                     </td>\n                  </tr>\n                  <tr style=\" \">\n                     <td colspan=\"2\">\n                        {% set batch_number = frappe.get_doc(\"Serial and Batch Bundle\", item.serial_and_batch_bundle) %}\n                        <table\n                           style=\"width:100%; border-collapse: collapse; margin-top: -13px; margin-left: -12px; margin-bottom: -13px;\">\n                           {% set count = namespace(repeat=0) %}\n                           <tr style=\"display:flex;flex-wrap:wrap; width:103.6%;\">\n                              {% for row in batch_number.entries %}\n                              {% set count.repeat = count.repeat + 1 %}\n                              <td style=\"width:16.65%;border:1px solid black;padding:2px 4px; margin-top: 0px;\">{{\n                                 row.serial_no }}</td>\n                              {% if count.repeat % 6 == 0 %}\n                           </tr>\n                           <!--{% if count.repeat % 20 == 0 %}-->\n                           <!--    <tr style=\"display:flex;flex-wrap:wrap;\">-->\n                           <!--        <td colspan=\"6\" style=\"border: none;\"></td>-->\n                           <!--    </tr>-->\n                           <!--{% endif %}-->\n                           <tr style=\"display:flex;flex-wrap:wrap; width:103.6%;\">\n                              {% endif %}\n                              {% endfor %}\n                              {% if count.repeat % 6 != 0 %}\n                              {% for _ in range(6 - count.repeat % 6) %}\n                              <td style=\"width:16.65%;border: none;\"></td>\n                              {% endfor %}\n                           </tr>\n                           {% endif %}\n                  </tr>\n               </table>\n            </td>\n            </tr>\n            {% endif %}\n            {% endfor %}\n   </table>\n   </td>\n   </tbody>\n</div>\n\n\n<!-- Table Footer Section -->\n<tfoot>\n   <tr>\n      <td>\n         <!-- Placeholder for the fixed-position footer -->\n         <div class=\"page-footer-space\"></div>\n      </td>\n   </tr>\n</tfoot>\n</table>\n\n</div>",
